# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0
REACT_APP_APP_NAME=DevResume Forge

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_PDF_EXPORT=true
REACT_APP_ENABLE_TEMPLATES=true

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf

# Resume Builder Settings
REACT_APP_AUTO_SAVE_INTERVAL=10000
REACT_APP_MAX_RESUMES_PER_USER=10
REACT_APP_DEFAULT_TEMPLATE=professional

# External Services (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=
REACT_APP_INTERCOM_APP_ID=

# Development Settings
REACT_APP_MOCK_API=false
REACT_APP_SHOW_DEV_TOOLS=true 